  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.9.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "aws-sdk": "^2.49.0",
    "chai": "^4.2.0",
