{
  "name": "v8-sandbox",
  "version": "2.3.0",
  "description": "V8 Sandbox",
  "homepage": "http://github.com/fulcrumapp/v8-sandbox",
  "main": "dist/index.js",
  "scripts": {
    "configure": "node-gyp configure",
    "build": "node-gyp configure build && tsc",
    "build-debug": "node-gyp --debug configure build && tsc",
    "rebuild": "node-gyp configure rebuild",
    "test": "TS_NODE_IGNORE='node_modules,dist' mocha -r ts-node/register test/index.js",
    "clean": "rm -rf dist build",
    "lint": "eslint 'lib/**/*.js'",
    "install": "node-pre-gyp install --fallback-to-build"
  },
  "author": "Zac McCormick <zac.mccormick@gmail.com> (http://github.com/zhm)",
  "license": "BSD",
  "repository": {
    "type": "git",
    "url": "git://github.com/fulcrumapp/v8-sandbox.git"
  },
  "eslintConfig": {
    "extends": "fulcrum"
  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/lodash": "^4.14.149",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "mocha": "^10.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "async": "^3.2.0",
    "bindings": "^1.5.0",
    "lodash": "^4.17.21",
    "nan": "^2.14.0",
    "node-gyp": "^9.0.0",
    "node-pre-gyp": "^0.17.0",
    "request": "^2.88.2",
    "signal-exit": "^3.0.2"
  },
  "binary": {
    "module_name": "sandbox",
    "module_path": "./build/Release/",
    "host": "https://zhm.s3.amazonaws.com/v8-sandbox/"
  },
  "keywords": [
    "sandbox",
    "v8",
    "js",
    "javascript"
  ]
}
